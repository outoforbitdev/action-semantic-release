{
  "branches": [
    'main',
    {
      "name": 'beta',
      "prerelease": true,
      "channel": 'beta'
    },
    {
      "name": 'alpha',
      "prerelease": true,
      "channel": 'alpha'
    },
    {
      "name": 'staging',
      "prerelease": true,
      "channel": 'rc'
    }
  ],
  "plugins": [
    '@semantic-release/commit-analyzer',
    '@semantic-release/release-notes-generator',
    ['@semantic-release/exec',
    {
      "analyzeCommitsCmd": 'export LAST_VERSION=${lastRelease.version}',
      "verifyReleaseCmd": 'export NEXT_VERSION=${nextRelease.version}; export VERSION_RELEASED=true'
    }],
    '@semantic-release/github'
  ]
}