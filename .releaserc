{
  "branches": [
    "main",
    {
      "name": "beta",
      "prerelease": true
    },
    {
      "name": "alpha",
      "prerelease": true
    },
    {
      "name": "dev",
      "prerelease": true
    },
    {
      "name": "staging",
      "prerelease": true,
      "channel": "rc"
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    ["@semantic-release/exec",
    {
      "analyzeCommitsCmd": "echo \"lastVersion=${lastRelease.version}\" >> $GITHUB_OUTPUT",
      "verifyReleaseCmd": "echo \"nextVersion=${nextRelease.version}\" >> $GITHUB_OUTPUT\necho \"versionReleased=true\" >> $GITHUB_OUTPUT"
    }],
    "@semantic-release/github",
    "@semantic-release/git"
  ]
}
